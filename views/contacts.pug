doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Contacts
        .cards 
            each item in data
                .card 
                    h2.card__name #{item.properties.nombre_del_proyecto} #{item.properties.tipo_de_proyecto}
                    p.card__detail #{item.properties.gerente_del_proyecto}
        a(href='/update-cobj') Create or Update Custom Object       